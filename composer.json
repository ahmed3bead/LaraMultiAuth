{
    "name": "ahmedebead/laramultiauth",
    "description": "A comprehensive authentication package for Laravel that supports multi-guard and OTP verification.",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Ebead",
            "email": "ahmed.ebead@munjz.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AhmedEbead\\LaraMultiAuth\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan multiauth:setup"
        ],
        "post-update-cmd": [
            "php artisan multiauth:setup"
        ]
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "ichtrojan/laravel-otp": "^2.0",
        "laravel/passport": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AhmedEbead\\LaraMultiAuth\\Providers\\LaraMultiAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
