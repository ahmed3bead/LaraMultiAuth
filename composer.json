{
    "name": "ahmedebead/laramultiauth",
    "description": "A comprehensive authentication package for Laravel.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "ichtrojan/laravel-otp": "^2.0",
        "laravel/passport": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "AhmedEbead\\LaraMultiAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AhmedEbead\\LaraMultiAuth\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "AhmedEbead\\LaraMultiAuth\\LaraMultiAuthServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan multiauth:setup"
        ],
        "post-update-cmd": [
            "@php artisan multiauth:setup"
        ]
    }
}
